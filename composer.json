{
	"name": "syrup/component-bundle",
	"type": "symfony-bundle",
	"license": "MIT",
	"description": "Syrup ComponentBundle",
	"keywords": [],
	"authors": [
		{
			"name": "Miroslav Čillík",
			"email": "miro@keboola.com"
		}
	],
	"repositories": [],
	"require": {
		"php": ">=5.3.5",
		"keboola/storage-api-client": "~2.10.0",
		"guzzle/guzzle": ">=3.0.0",
		"keboola/csv": "1.1.*",
		"aws/aws-sdk-php": "2.*",
		"keboola/php-encryption": "~0.2.0"
	},
	"require-dev": {
		"keboola/syrup": "~1.2.0",
		"phpunit/phpunit": "3.7.*"
	},
	"scripts": {
		"post-install-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"./vendor/sensio/distribution-bundle/Sensio/Bundle/DistributionBundle/Resources/bin/build_bootstrap.php vendor/syrup/development-bundle/app"
		],
		"post-update-cmd": [
			"./vendor/sensio/distribution-bundle/Sensio/Bundle/DistributionBundle/Resources/bin/build_bootstrap.php vendor/syrup/development-bundle/app"
		]
	},
	"minimum-stability": "stable",
	"extra": {
		"incenteev-parameters": {
			"keep-outdated": true,
			"file": "vendor/syrup/development-bundle/app/config/parameters.yml",
			"dist-file": "parameters.yml.dist"
		}
	},
	"autoload": {
		"psr-0": { "Syrup\\ComponentBundle": "" }
	},
	"target-dir": "Syrup/ComponentBundle"
}
