{
	"name": "syrup/component-bundle",
	"type": "symfony-bundle",
	"description": "Syrup ComponentBundle",
	"keywords": [],
	"authors": [
		{
			"name": "Miroslav Čillík",
			"email": "miro@keboola.com"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@bitbucket.org:keboola/syrup-development-bundle.git"
		}
	],
	"require": {
		"php": ">=5.3.5",
		"keboola/storage-api-client": "2.6.*",
		"guzzle/guzzle": ">=3.0.0",
		"keboola/csv": "1.0.*",
		"aws/aws-sdk-php": "2.*"
	},
	"require-dev": {
		"syrup/development-bundle": ">=1.0.8"
	},
	"scripts": {
		"post-install-cmd": [
			"./vendor/sensio/distribution-bundle/Sensio/Bundle/DistributionBundle/Resources/bin/build_bootstrap.php vendor/syrup/development-bundle/app"
		],
		"post-update-cmd": [
			"./vendor/sensio/distribution-bundle/Sensio/Bundle/DistributionBundle/Resources/bin/build_bootstrap.php vendor/syrup/development-bundle/app"
		]
	},
	"minimum-stability": "alpha",
	"extra": {
		"branch-alias": {
			"dev-master": "2.2-dev"
		}
	},
	"autoload": {
		"psr-0": { "Syrup\\ComponentBundle": "" }
	},
	"target-dir": "Syrup/ComponentBundle"
}
